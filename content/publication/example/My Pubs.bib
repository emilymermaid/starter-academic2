
@inproceedings{duan_let_2018,
	location = {Montreal {QC}, Canada},
	title = {"Let Me Ask Them to Clarify If You Don't Want To" - A Clarification Agent for Nonnative Speakers},
	rights = {{CC}0 1.0 Universal Public Domain Dedication},
	isbn = {978-1-4503-5621-3},
	url = {http://dl.acm.org/citation.cfm?doid=3170427.3188600},
	doi = {10.1145/3170427.3188600},
	abstract = {When non-native English speakers ({NNS}) encounter messages they do not understand, they are often reluctant to ask native speakers ({NS}) for clarification. In this paper, we explored whether a conversation agent that asks clarification questions would increase {NNS}’ willingness to ask questions. We compared two agents: one that asked for clarification about specific message elements and one that asked general clarification questions. {NNS} and {NS} rated how disruptive the agent was, the quality of the conversation, and whether they would feel embarrassed to ask their own questions. {NNS} found both types of agent less disruptive than {NS} did, but both found the specific agent more disruptive than the generic agent. {NS} rated the conversations higher in quality than {NNS}, but there was no effect of agent condition. We discuss potential of using conversational agents to boost {NNS}’s confidence in conversation.},
	eventtitle = {Extended Abstracts of the 2018 {CHI} Conference},
	pages = {1--6},
	booktitle = {Extended Abstracts of the 2018 {CHI} Conference on Human Factors in Computing Systems - {CHI} '18},
	publisher = {{ACM} Press},
	author = {Duan, Wen and Yamashita, Naomi and Hwang, Sun Young and Fussell, Susan},
	urldate = {2019-11-01},
	date = {2018},
	langid = {english},
	file = {Duan et al. - 2018 - Let Me Ask Them to Clarify If You Don't Want To .pdf:/Users/duanwen/Zotero/storage/FDVK5DFS/Duan et al. - 2018 - Let Me Ask Them to Clarify If You Don't Want To .pdf:application/pdf}
}

@article{bell_failed_2010,
	title = {Failed humor: Issues in non-native speakers' appreciation and understanding of humor},
	volume = {7},
	issn = {1612-295X, 1613-365X},
	url = {https://www.degruyter.com/view/j/iprg.2010.7.issue-3/iprg.2010.019/iprg.2010.019.xml},
	doi = {10.1515/iprg.2010.019},
	shorttitle = {Failed humor},
	abstract = {What is it that makes humor difficult to understand and appreciate in a second language (L2)? Despite advances in research in both L2 pragmatics and humor studies, scholars have as yet had little to say on this topic. In interviews, learners themselves pinpoint culture, vocabulary, and the speed at which playful talk often takes place as difficulties. Yet, as we know, hypothetical selfreports of language use can be quite unreliable. Thus, this exploratory study aimed to examine L2 understanding of humor more systematically. Six advanced non-native speakers ({NNSs}) of English kept diaries in which they recorded their experiences with humor in English over an eight-week period. Group meetings were also held every two weeks during this time to allow the participants to elaborate on, interpret, and discuss their experiences. For the present study, all instances of failed humor were extracted and coded. These were used to construct a typology of failed humor, which identifies seven levels at which a speaker may fail to successfully engage in a humorous exchange. The data suggest that {NNS} failures differ from those of {NSs} largely from a quantitative, but not qualitative, standpoint.},
	number = {3},
	journaltitle = {Intercultural Pragmatics},
	author = {Bell, Nancy and Attardo, Salvatore},
	urldate = {2019-11-19},
	date = {2010-01},
	langid = {english},
	file = {[Intercultural Pragmatics] Failed humor_ Issues in non-native speakers' appreciation and understanding of humor.pdf:/Users/duanwen/Documents/LITERATURE/sort by topic/Humor/Lit Humor/failed humor and imbalance production and appreciation/[Intercultural Pragmatics] Failed humor_ Issues in non-native speakers' appreciation and understanding of humor.pdf:application/pdf}
}

@article{moran_i_2014,
	title = {Do I amuse you? Asymmetric predictors for humor appreciation and humor production},
	volume = {49},
	issn = {00926566},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0092656613001475},
	doi = {10.1016/j.jrp.2013.12.002},
	shorttitle = {Do I amuse you?},
	abstract = {A ‘‘sense of humor’’ can be fractionated into appreciation (enjoying jokes), production ﬂuency (making jokes), and production success (making funny jokes). There is scant research on how appreciation and production relate, and their relation to individual differences. Participants (N = 159) rated the humor of captioned cartoons and created captions for different cartoons. People who wrote funnier captions were less amused by the professionally-captioned cartoons. Production ﬂuency, in contrast, was not related to appreciation. Personality predicted humor appreciation, but not production success. Demographics predicted production success, but not appreciation. Appreciation and production success appear to rely on separable mechanisms and motivations. Our results were also inconsistent with the idea that humor creators are motivated by dominance and humor appreciators by afﬁliation.},
	pages = {8--13},
	journaltitle = {Journal of Research in Personality},
	shortjournal = {Journal of Research in Personality},
	author = {Moran, Joseph M. and Rain, Marina and Page-Gould, Elizabeth and Mar, Raymond A.},
	urldate = {2019-11-19},
	date = {2014-04},
	langid = {english},
	file = {Do I amuse you? Asymmetric predictors for humor appreciation and humor production.pdf:/Users/duanwen/Documents/LITERATURE/sort by topic/Humor/Lit Humor/failed humor and imbalance production and appreciation/Do I amuse you? Asymmetric predictors for humor appreciation and humor production.pdf:application/pdf}
}

@article{bell_responses_2009,
	title = {Responses to failed humor},
	volume = {41},
	issn = {03782166},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0378216608002749},
	doi = {10.1016/j.pragma.2008.10.010},
	abstract = {Although research on the pragmatics of humor has increased, the study of failed humor has continued to be neglected. The purpose of the present paper is to open the discussion of this phenomenon through the examination of a corpus of 186 responses to failed humor. These responses, which were elicited through the telling of a joke found to be lacking in humor for most hearers, were classiﬁed by type and affective sense (positive, negative, and neutral). The data were also categorized by the sociolinguistic variables of age, gender, and social relationship. The ﬁndings indicate that responses to failed humor go well beyond the typical groans or fake laughter, that are often asserted as prototypical reactions in the literature, with laughter, metalinguistic comments, and interjections as the most common responses. In addition, no signiﬁcant differences were found in response types according to age or gender; however, very strong effects were found for social relationship, with negative reactions more common among intimates and neutral reactions preferred by acquaintances and strangers.},
	pages = {1825--1836},
	number = {9},
	journaltitle = {Journal of Pragmatics},
	shortjournal = {Journal of Pragmatics},
	author = {Bell, Nancy D.},
	urldate = {2019-11-19},
	date = {2009-09},
	langid = {english},
	file = {Responses to failed humor.pdf:/Users/duanwen/Documents/LITERATURE/sort by topic/Humor/Lit Humor/failed humor and imbalance production and appreciation/Responses to failed humor.pdf:application/pdf}
}

@article{bell_responses_2013,
	title = {Responses to incomprehensible humor},
	volume = {57},
	issn = {03782166},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0378216613002099},
	doi = {10.1016/j.pragma.2013.08.019},
	abstract = {Humor may fail for many reasons and the ways in which this happens are just beginning to be explored. Bell (2009a,b) used an unfunny joke to elicit responses in order to describe reactions that an attempt at humor receives when the joke is not appreciated. The present paper adopts those methods to explore responses to a joke that failed because it was not understood by the recipient. A total of 278 responses were elicited using a joke that was impossible for most hearers to comprehend without explanation. The most common reactions included non-verbal responses, explicit expressions of non-understanding (e.g. ‘‘I don’t get it’’), laughter, silence, and repetition of the punch line. Significant differences were found in examining the responses by the sociolinguistic variables of gender and social relationship, but not age.},
	pages = {176--189},
	journaltitle = {Journal of Pragmatics},
	shortjournal = {Journal of Pragmatics},
	author = {Bell, Nancy D.},
	urldate = {2019-11-19},
	date = {2013-10},
	langid = {english},
	file = {Responses to incomprehensible humor.pdf:/Users/duanwen/Documents/LITERATURE/sort by topic/Humor/Lit Humor/failed humor and imbalance production and appreciation/Responses to incomprehensible humor.pdf:application/pdf}
}

