@article{bell_how_2007,
	title = {How native and non-native English speakers adapt to humor in intercultural interaction},
	volume = {20},
	issn = {0933-1719, 1613-3722},
	url = {https://www.degruyter.com/view/j/humr.2007.20.issue-1/humor.2007.002/humor.2007.002.xml},
	doi = {10.1515/HUMOR.2007.002},
	abstract = {Humor can often carry an implicit negative message and thus be potentially dangerous to use. In addition, it is culturally and linguistically complex and sophisticated. Because of these things, it poses a challenge for L2 (second language) speakers and we might expect to see attempts at humor failing and causing o¤ense in intercultural interaction. This paper reports on a study that examined humor in interaction between native and non-native speakers of English and found that humor did not seem to be a cause of conﬂict because of adjustments speakers made to their speech and their situated interpretations of meaning. In general, taboo topics and potentially dangerous forms of humor were avoided and humor was carefully contextualized. Native speakers reported being careful about the vocabulary they used in creating humor and both sides appeared to approach humor in intercultural communication prepared to accommodate the other and with an attitude of leniency.},
	number = {1},
	journaltitle = {Humor – International Journal of Humor Research},
	author = {Bell, Nancy D},
	urldate = {2019-11-19},
	date = {2007-01-20},
	langid = {english},
	file = {[IC] How native and non-native English speakers adapt to humor in intercultural interaction.pdf:/Users/duanwen/Documents/LITERATURE/sort by topic/Humor/Lit Humor/IC & humor/[IC] How native and non-native English speakers adapt to humor in intercultural interaction.pdf:application/pdf}
}

