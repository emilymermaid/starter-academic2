@inproceedings{handel_what_2002,
	title = {What Is Chat Doing in the Workplace?},
	abstract = {We report an empirical study of a synchronous messaging application with group-oriented functionality designed to support teams in the workplace. In particular, the tool supports group chat windows that allow members of a group to communicate with text that persists for about a day. We describe the experience of 6 globally-distributed work groups who used the tool over a period of 17 months. An analysis of use shows that the group functionality was used primarily for bursts of synchronous conversations and occasional asynchronous exchanges. The content was primarily focused on work tasks, and negotiating availability, with a smattering of non-work topics and humor. Nearly all groups were remarkably similar in the content of their group chat, although the research group chatted far more frequently than the others. We conclude with suggestions for future research, and a discussion of the place of team-oriented synchronous messaging tools in the workplace.},
	pages = {10},
	booktitle = {{CSCW}'02},
	author = {Handel, Mark and Herbsleb, James D},
	date = {2002},
	langid = {english},
	file = {What Is Chat Doing in the Workplace.pdf:/Users/duanwen/Documents/LITERATURE/sort by topic/Humor/Lit Humor/humor @ workplace/What Is Chat Doing in the Workplace.pdf:application/pdf}
}

@article{bell_humor_2007,
	title = {Humor comprehension: Lessons learned from cross-cultural communication},
	volume = {20},
	issn = {0933-1719, 1613-3722},
	url = {https://www.degruyter.com/view/j/humr.2007.20.issue-4/humor.2007.018/humor.2007.018.xml},
	doi = {10.1515/HUMOR.2007.018},
	shorttitle = {Humor comprehension},
	abstract = {Research on humor has tended to focus on its production. The understanding of humor, perhaps because it is more difficult to document, has received somewhat less attention, especially from a sociolinguistic perspective. The present paper addresses this gap by using examples of humor in crosscultural interaction to propose ways in which two earlier models suggesting how humor is perceived (Carrell 1997) and supported (Hay 2001) may be further developed. These examples show that joke competence is a somewhat dynamic construct, that understanding can be partial and that lack of full understanding need not preclude appreciation. In addition, I suggest that interlocutors’ understanding of the wide variety of functions of humor must also be taken into consideration in any broader theory of how humor is perceived and understood. As understanding, like language use, is interactively constructed, I call for a view of humor comprehension that is social, as well as cognitive.},
	number = {4},
	journaltitle = {Humor – International Journal of Humor Research},
	author = {Bell, Nancy D},
	urldate = {2019-11-19},
	date = {2007-01-20},
	langid = {english},
	file = {[HUMOR] Humor comprehension_ Lessons learned from cross-cultural communication.pdf:/Users/duanwen/Documents/LITERATURE/sort by topic/Humor/Lit Humor/IC & humor/[HUMOR] Humor comprehension_ Lessons learned from cross-cultural communication.pdf:application/pdf}
}

