@article{blanchard_making_2014,
	title = {Making sense of humor at work.},
	volume = {17},
	issn = {1550-3461, 1088-7156},
	url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/mgr0000011},
	doi = {10.1037/mgr0000011},
	abstract = {Humor is a very common, but understudied phenomenon at work. In this study, we examine humor climate as a form of employee sensemaking. That is, employee humor climate provides an identiﬁable way in which employees interpret ambiguous events to make meaning of their organizations and their place within it. We develop a model in which role ambiguity and perceived organizational support ({POS}) interact to affect positive and negative humor climate at work. Further, we propose that managerial support for humor mediates the relationship between {POS} and employees’ positive and negative humor climate. Finally, we hypothesize that as a way of making sense of the organization, humor climate should be related to organizational identiﬁcation. We collected data on 373 full-time employees in a variety of organizations. Using {SEM}, the results generally support our model. The important managerial implications of studying humor as a form of employee sensemaking are discussed.},
	pages = {49--70},
	number = {1},
	journaltitle = {The Psychologist-Manager Journal},
	shortjournal = {The Psychologist-Manager Journal},
	author = {Blanchard, Anita L. and Stewart, Oscar J. and Cann, Arnie and Follman, Leslie},
	urldate = {2019-11-19},
	date = {2014-02},
	langid = {english},
	file = {Making sensen of humor at work.pdf:/Users/duanwen/Documents/LITERATURE/sort by topic/Humor/Lit Humor/humor @ workplace/Making sensen of humor at work.pdf:application/pdf}
}

