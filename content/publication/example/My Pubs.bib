	langid = {english},
	file = {Liddell and Williams - 2019 - Cultural Differences in Interpersonal Emotion Regu.pdf:/Users/duanwen/Zotero/storage/375FDBVR/Liddell and Williams - 2019 - Cultural Differences in Interpersonal Emotion Regu.pdf:application/pdf}
}

